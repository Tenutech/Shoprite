{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2.4",
        "aacotroneo/laravel-saml2": "^2.1",
        "fruitcake/php-cors": "^1.2",
        "guzzlehttp/guzzle": "^7.8",
        "intervention/image": "^2",
        "laravel/framework": "^10.14",
        "laravel/sanctum": "^3.2",
        "laravel/telescope": "^5.1",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-activitylog": "^4.7",
        "tightenco/ziggy": "^1.6",
        "twilio/sdk": "^7.11"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^6.11",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.17",
        "pestphp/pest-plugin-laravel": "^2.2",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "coverage": [
            "rm -rf build/coverage || :",
            "XDEBUG_MODE=coverage composer exec phpunit"
        ],
        "tests": [
            "@php artisan test"
        ],
        "lint": "phpcs --standard=phpcs.xml --colors app/",
        "lint-fix": "phpcbf --standard=phpcs.xml app/",
        "lint-short": "phpcs --standard=phpcs.xml --report=summary --colors app/",
        "style": "php-cs-fixer fix --dry-run --diff"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.jp"
        }
    }
}
